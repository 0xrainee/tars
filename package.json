{
  "name": "tars-cli",
  "version": "0.1.1",
  "description": "AI-powered CLI coding assistant",
  "bin": {
    "tars": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node --minify",
    "postbuild": "echo '#!/usr/bin/env node' | cat - dist/index.js > tmp && mv tmp dist/index.js && chmod +x dist/index.js",
    "prepublishOnly": "bun run build",
    "start": "node dist/index.js",
    "lint": "tsc --noEmit"
  },
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.20",
    "@clack/prompts": "^1.0.0",
    "ai": "^6.0.67",
    "cfonts": "^3.3.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "zod": "^4.3.6"
  }
}
